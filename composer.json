{
    "name": "webcien/kardocss",
    "description": "Framework CSS moderno, mobile-first y ultra-ligero con Dark Mode y PurgeCSS integrados",
    "type": "library",
    "keywords": [
        "css",
        "framework",
        "utility-first",
        "mobile-first",
        "responsive",
        "dark-mode",
        "tailwind-alternative",
        "kardocss",
        "css-framework",
        "frontend"
    ],
    "homepage": "https://github.com/webcien/KardoCSS",
    "license": "MIT",
    "authors": [
        {
            "name": "Juan Quezada",
            "email": "juan@webcien.com",
            "homepage": "https://github.com/webcien",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/webcien/KardoCSS/issues",
        "source": "https://github.com/webcien/KardoCSS",
        "docs": "https://github.com/webcien/KardoCSS#readme"
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "WebCien\\KardoCSS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebCien\\KardoCSS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.1.x-dev"
        },
        "version": "1.1.3"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

